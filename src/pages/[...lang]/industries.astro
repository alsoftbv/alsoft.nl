---
import { industries } from "@data/industries";
import SectionsLayout from "@layouts/SectionsLayout.astro";
import { getTranslationPaths, useTranslations } from "@locales/utils";

export function getStaticPaths() {
  return getTranslationPaths();
}

const t = useTranslations(Astro.params.lang);
---

<SectionsLayout
  title={t.industries.title}
  description={t.industries.description}
  pageTitle={t.industries.header}
  data={industries}
/>
